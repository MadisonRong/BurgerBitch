<%= form_for(@user, method: :patch) do |f| %>
  <div class="panel panel-primary">
    <div class="panel-heading">完善资料</div>
    <div class="panel-body">
      <p>请填写你的真实名称：<%= f.text_field :real_name %></p>
    </div>
    <div class="panel-footer">
      <p><%= f.submit "提交", :disable_with => '提交ing...' %></p>
    </div>
  </div>
<% end %>